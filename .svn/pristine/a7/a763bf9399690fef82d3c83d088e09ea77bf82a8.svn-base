package nsl.orion.crankshaftdeflectiongauge.common;

import java.sql.Time;

/**
 * Created by TienNT on 9/6/2015.
 */
public class Cylinder {

    public static final int POSITION_T = 1;
    public static final int POSITION_P = 2;
    public static final int POSITION_BP = 3;
    public static final int POSITION_BE = 4;
    public static final int POSITION_E = 5;

    private Long engineId;
    private Integer order;
    private Float t;
    private Float p;
    private Float bp;
    private Float be;
    private Float e;
    private Time dateOfCreation;
    private Time lastUpdate;

    public Long getEngineId() {
        return engineId;
    }

    public void setEngineId(long engineId) {
        this.engineId = engineId;
    }

    public Integer getOrder() {
        return order;
    }

    public void setOrder(int order) {
        this.order = order;
    }

    public Float getT() {
        return t;
    }

    public void setT(float t) {
        this.t = t;
    }

    public Float getP() {
        return p;
    }

    public void setP(float p) {
        this.p = p;
    }

    public Float getBp() {
        return bp;
    }

    public void setBp(float bp) {
        this.bp = bp;
    }

    public Float getBe() {
        return be;
    }

    public void setBe(float be) {
        this.be = be;
    }

    public Float getE() {
        return e;
    }

    public void setE(float e) {
        this.e = e;
    }

    public Time getDateOfCreation() {
        return dateOfCreation;
    }

    public void setDateOfCreation(Time dateOfCreation) {
        this.dateOfCreation = dateOfCreation;
    }

    public Time getLastUpdate() {
        return lastUpdate;
    }

    public void setLastUpdate(Time lastUpdate) {
        this.lastUpdate = lastUpdate;
    }
}
